{
  "expo": {
    "name": "IRS Notice Tracker",
    "slug": "irs-notice-tracker-expo",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "scheme": "irsnoticetracker",
    "platforms": ["ios", "android", "web"],
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.irsnoticetracker"
    },
    "android": {
      "package": "com.yourcompany.irsnoticetracker"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}